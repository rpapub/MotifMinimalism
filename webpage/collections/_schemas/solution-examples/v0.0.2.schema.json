{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://rpapub.github.io/MotifMinimalism/schemas/solution-examples/v0.0.2.schema.json",
  "title": "Solution Examples",
  "type": "object",
  "properties": {
    "schema_version": {
      "type": "string"
    },
    "solution_examples": {
      "type": "object",
      "patternProperties": {
        "^[a-f0-9\\-]{36}$": {
          "$schema": "https://json-schema.org/draft/2020-12/schema",
          "type": "object",
          "properties": {
            "CreatedAt": {
              "type": "string"
            },
            "Name": {
              "type": "string"
            },
            "educational": {
              "type": "object",
              "properties": {
                "estimated_time_minutes": {
                  "type": "integer"
                },
                "goals": {
                  "type": "array"
                },
                "level": {
                  "type": "string"
                },
                "prerequisites": {
                  "type": "array"
                }
              },
              "required": [
                "estimated_time_minutes",
                "goals",
                "level",
                "prerequisites"
              ]
            },
            "publishing": {
              "type": "object",
              "properties": {
                "audience": {
                  "type": "array"
                },
                "authors": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "created": {
                  "type": "string"
                },
                "last_updated": {
                  "type": "string"
                },
                "render": {
                  "type": "object",
                  "properties": {
                    "layout": {
                      "type": "string"
                    },
                    "permalink": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "layout",
                    "permalink"
                  ]
                },
                "slug": {
                  "type": "string"
                },
                "status": {
                  "type": "string"
                },
                "summary": {
                  "type": "string"
                },
                "tags": {
                  "type": "array"
                },
                "title": {
                  "type": "string"
                },
                "visibility": {
                  "type": "string"
                }
              },
              "required": [
                "audience",
                "authors",
                "created",
                "last_updated",
                "render",
                "slug",
                "status",
                "summary",
                "tags",
                "title",
                "visibility"
              ]
            },
            "runtime_construct": {
              "type": "object",
              "properties": {
                "artifact_version": {
                  "type": "string"
                },
                "category": {
                  "type": "string"
                },
                "compatibility": {
                  "type": "string"
                },
                "design_tool_versions": {
                  "type": "array"
                },
                "edge_cases": {
                  "type": "array"
                },
                "execution_phase": {
                  "type": "string"
                },
                "inputs": {
                  "type": "string"
                },
                "observability": {
                  "type": "string"
                },
                "outputs": {
                  "type": "string"
                },
                "platform_constraints": {
                  "type": "string"
                },
                "runtime_versions": {
                  "type": "array"
                },
                "source_commit": {
                  "type": "string"
                },
                "tags": {
                  "type": "array"
                },
                "tech_stack": {
                  "type": "array"
                }
              },
              "required": [
                "artifact_version",
                "category",
                "compatibility",
                "design_tool_versions",
                "edge_cases",
                "execution_phase",
                "inputs",
                "observability",
                "outputs",
                "platform_constraints",
                "runtime_versions",
                "source_commit",
                "tags",
                "tech_stack"
              ]
            },
            "solution": {
              "type": "object",
              "properties": {
                "Authors": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "Description": {
                  "type": "string"
                },
                "DocVersion": {
                  "type": "string"
                },
                "DownloadUrls": {
                  "type": "array"
                },
                "Name": {
                  "type": "string"
                },
                "Projects": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "Type": {
                        "type": "string"
                      },
                      "ProjectRelativePath": {
                        "type": "string"
                      },
                      "Id": {
                        "type": "string"
                      },
                      "Name": {
                        "type": "string"
                      },
                      "Description": {
                        "type": "string"
                      },
                      "images": {
                        "type": "array"
                      },
                      "inputs": {
                        "type": "object",
                        "additionalProperties": true
                      },
                      "outputs": {
                        "type": "object",
                        "additionalProperties": true
                      }
                    },
                    "required": [
                      "Description",
                      "Id",
                      "Name",
                      "ProjectRelativePath",
                      "Type",
                      "images"
                    ]
                  }
                },
                "SolutionId": {
                  "type": "string"
                },
                "StudioMinVersion": {
                  "type": "string"
                },
                "created_at": {
                  "type": "string"
                },
                "images1": {
                  "type": "array"
                },
                "tech_stack": {
                  "type": "object",
                  "properties": {
                    "account_type": {
                      "type": "string"
                    },
                    "design_tool_versions": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "name": {
                      "type": "string"
                    },
                    "orchestrator_version": {
                      "type": "string"
                    },
                    "studio_version": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "account_type",
                    "design_tool_versions",
                    "name",
                    "orchestrator_version",
                    "studio_version"
                  ]
                }
              },
              "required": [
                "Authors",
                "Description",
                "DocVersion",
                "DownloadUrls",
                "Name",
                "Projects",
                "SolutionId",
                "StudioMinVersion",
                "created_at",
                "images1",
                "tech_stack"
              ]
            }
          },
          "required": [
            "CreatedAt",
            "Name",
            "educational",
            "publishing",
            "runtime_construct",
            "solution"
          ]
        }
      },
      "additionalProperties": false
    }
  },
  "required": [
    "schema_version",
    "solution_examples"
  ]
}